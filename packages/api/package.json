{
	"name": "@node-zugferd/api",
	"version": "0.0.5",
	"type": "module",
	"scripts": {
		"build": "tsup --clean --dts",
		"dev": "tsup --watch --sourcemap",
		"dev:dts": "tsup --watch --dts",
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"author": "JoÃ©l de Oliveira Solano da Silva",
	"repository": {
		"type": "git",
		"url": "https://github.com/jslno/node-zugferd.git",
		"directory": "packages/api"
	},
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.10.7",
		"@types/react": "^19.0.9",
		"@types/react-dom": "^19.0.3",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"solid-js": "^1.9.5",
		"svelte": "^5.25.2",
		"tsup": "^8.3.5",
		"typescript": "^5.7.3",
		"vitest": "^3.0.4",
		"vue": "^3.5.13"
	},
	"dependencies": {
		"@better-fetch/fetch": "^1.1.17",
		"better-call": "^1.0.5",
		"node-zugferd": "workspace:*",
		"puppeteer": "^24.4.0",
		"zod": "^3.24.1"
	},
	"peerDependencies": {
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"solid-js": "^1.9.5",
		"svelte": "^5.25.2",
		"vue": "^3.5.13"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./react/renderer": {
			"import": {
				"types": "./dist/react/renderer.d.ts",
				"default": "./dist/react/renderer.js"
			},
			"require": {
				"types": "./dist/react/renderer.d.cts",
				"default": "./dist/react/renderer.cjs"
			}
		},
		"./vue/renderer": {
			"import": {
				"types": "./dist/vue/renderer.d.ts",
				"default": "./dist/vue/renderer.js"
			},
			"require": {
				"types": "./dist/vue/renderer.d.cts",
				"default": "./dist/vue/renderer.cjs"
			}
		},
		"./solid/renderer": {
			"import": {
				"types": "./dist/solid/renderer.d.ts",
				"default": "./dist/solid/renderer.js"
			},
			"require": {
				"types": "./dist/solid/renderer.d.cts",
				"default": "./dist/solid/renderer.cjs"
			}
		},
		"./svelte/renderer": {
			"import": {
				"types": "./dist/svelte/renderer.d.ts",
				"default": "./dist/svelte/renderer.js"
			},
			"require": {
				"types": "./dist/svelte/renderer.d.cts",
				"default": "./dist/svelte/renderer.cjs"
			}
		},
		"./client": {
			"import": {
				"types": "./dist/client.d.ts",
				"default": "./dist/client.js"
			},
			"require": {
				"types": "./dist/client.d.cts",
				"default": "./dist/client.cjs"
			}
		}
	}
}
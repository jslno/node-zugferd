{
  "name": "@node-zugferd/xsd",
  "version": "0.1.1-beta.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://node-zugferd.jsolano.de/docs/plugins/xsd",
  "repository": {
    "type": "git",
    "url": "https://github.com/jslno/node-zugferd",
    "directory": "packages/plugins/xsd"
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest",
    "dev": "tsdown --watch",
    "typecheck": "tsc --project tsconfig.json"
  },
  "exports": {
    ".": {
      "dev-source": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@node-zugferd/core": "workspace:*",
    "libxml2-wasm": "^0.6.0"
  },
  "devDependencies": {
    "@node-zugferd/test-utils": "workspace:*",
    "tsdown": "catalog:"
  },
  "files": [
    "schemas",
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}

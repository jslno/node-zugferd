{
  "name": "node-zugferd",
  "version": "0.1.1-beta.1",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "license": "MIT",
  "homepage": "https://node-zugferd.jsolano.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/better-auth/better-auth",
    "directory": "packages/better-auth"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --project tsconfig.json"
  },
  "exports": {
    ".": {
      "node-zugferd-dev-source": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@node-zugferd/core": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "tsdown": "catalog:"
  },
  "files": [
    "dist"
  ]
}

{
  "name": "node-zugferd",
  "version": "0.1.1-beta.1",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://node-zugferd.jsolano.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/jslno/node-zugferd",
    "directory": "packages/node-zugferd"
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest",
    "dev": "tsdown --watch",
    "typecheck": "tsc --project tsconfig.json"
  },
  "exports": {
    ".": {
      "dev-source": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./pdf": {
      "dev-source": "./src/pdf/index.ts",
      "import": {
        "types": "./dist/pdf/index.d.ts",
        "default": "./dist/pdf/index.js"
      },
      "require": {
        "types": "./dist/pdf/index.d.cts",
        "default": "./dist/pdf/index.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "./pdfa": [
        "dist/pdfa/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "defu": "catalog:",
    "@node-zugferd/core": "workspace:*",
    "@cantoo/pdf-lib": "catalog:",
    "uncrypto": "^0.1.3"
  },
  "devDependencies": {
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "files": [
    "dist"
  ]
}

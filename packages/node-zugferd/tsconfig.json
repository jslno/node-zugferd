{
	"compilerOptions": {
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"strictNullChecks": true,
		"target": "ES2022",
		"moduleResolution": "node",
		"module": "ES2022",
		"declaration": true,
		"isolatedModules": true,
		"noEmit": true,
		"outDir": "dist"
	},
	"include": ["src/**/*"],
	"exclude": ["**/dist", "node_modules"],
	"references": []
}
